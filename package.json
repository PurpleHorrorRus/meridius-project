    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.12.1",
    "gh-pages": "^3.2.3",
    "push-dir": "^0.4.1",
    "sass": "^1.35.1",
    "sass-loader": "10.1.1"
